@model IEnumerable<ProductDelivery.EF.Order>



<h2>Order History</h2>

<table>
    <tr>
        <th>Order Number</th>
        <th>Order Status</th>

    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.Id</td>
            <td>@order.Status</td>
        </tr>
    }
</table>



